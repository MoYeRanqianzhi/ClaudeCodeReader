{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    "fs:allow-read-dir",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-remove",
    "fs:allow-exists",
    "fs:allow-stat",
    "fs:allow-home-read-recursive",
    "fs:allow-home-write-recursive",
    "dialog:default",
    "shell:default",
    "opener:default"
  ]
}
